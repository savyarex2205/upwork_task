{
    "[python]": {
        "editor.defaultFormatter": "ms-python.charliermarsh.ruff",
        "editor.formatOnSave": true,
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "black-formatter.args": [
        "--line-length",
        "80"
    ],
    "python.autoComplete.extraPaths": [
        "./src"
    ],
    "python.analysis.extraPaths": [
        "./src"
    ],
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "files.autoSave": "onFocusChange",
    "cSpell.words": [
        "backtest",
        "bordercolor",
        "borderwidth",
        "Clauclate",
        "cumdev",
        "cumsum",
        "dataframe",
        "Defualt",
        "degross",
        "displaylogo",
        "Drawdowns",
        "expirdate",
        "fixedrange",
        "fromdate",
        "garman",
        "hoverinfo",
        "hovermode",
        "inital",
        "intrinio",
        "investmnet",
        "ivol",
        "klass",
        "modebar",
        "nbinsx",
        "obbh",
        "openbb",
        "ORATS",
        "Patito",
        "premia",
        "prepost",
        "quantile",
        "quantstats",
        "reqs",
        "rowspan",
        "satchell",
        "showarrow",
        "showgrid",
        "showlegend",
        "showline",
        "showspikes",
        "showticklabels",
        "spikecolor",
        "spikedistance",
        "Spikelines",
        "spikemode",
        "spikethickness",
        "todate",
        "togglespikelines",
        "tozeroy",
        "traceorder",
        "tradedate",
        "xanchor",
        "xaxes",
        "xaxis",
        "yaxes",
        "yref",
        "zeroline",
        "zerolinecolor",
        "zhang",
        "zoomin",
        "zoomout"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "workbench.activityBar.orientation": "vertical",
    "editor.inlineSuggest.showToolbar": "always"
}